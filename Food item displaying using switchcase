#include<stdio.h>
void main()
{
	int FoodItemNo;
	printf("1. Pizza\n2. Burger\n3. Pasta\n4. French Fries\n5. Sandwich\n");
	printf("Enter a Food Item number:");
	scanf("%d",&FoodItemNo);
	switch(FoodItemNo)
	{
		case 1:
			printf("Food item - Pizza\nPrice - Rs 239 ");
			break;
			
		case 2:
			printf("Food item - Burger\nPrice - Rs 129 ");
			break;
			
		case 3:
			printf("Food item - Pasta\nPrice - Rs 179 ");
			break;
			
		case 4:
			printf("Food item - Frech fries\nPrice - Rs 99 ");
			break;
		
		case 5:
			printf("Food item - Sandwich\nPrice - Rs 149 ");
			break;
			
		default : printf("Enter a valid Food Item number\n");
	}
}
